{
  "name": "remix-app-template-js",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development remix build && run-p dev:*",
    "dev:node": "npm run transpile && cross-env NODE_ENV=development nodemon ./build/server/server.js --watch server",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "postinstall": "remix setup node",
    "transpile": "babel ./server -d build/server",
    "clean": "rimraf dist-server",
    "start": "npm run transpile && cross-env NODE_ENV=production node ./build/server/server.js"
  },
  "dependencies": {
    "@remix-run/express": "^1.7.6",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase": "^9.6.10",
    "firebase-admin": "^10.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redis": "^4.3.1",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "babel-plugin-source-map-support": "^2.2.0",
    "eslint": "^8.9.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "node": ">=14"
  }
}
